package working;
import java.util.Scanner;
public class freq {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		char c;
		String s=new String();
		int count=0;
		System.out.println("enter sentence");
		s=sc.nextLine();
		System.out.println("enter alphbet");
		c=sc.next().charAt(0);
		
		int l=s.length();
		for(int i=0;i<l;i++)
		if(s.charAt(i)==c)
		{ count=count+1;
		
		}
		System.out.println("no of times" +c +"is"+count);
		
	}
	}
